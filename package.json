  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/iamEvanYT/electron-datastore",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "electron": "^28.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "electron": ">=11.0.0"
  }
}
